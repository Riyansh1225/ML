# Order of Sorting --> 


import pandas as pd
import numpy as np

a=pd.DataFrame(np.random.rand(10,5) , index=[2,3,4,5,6,7,8,9,1,10],
               columns=['Koti','Lakshmi','Satish','Phani','Bullodu'])
print (a)
b=a.sort_index(axis=1,ascending=False)
print ('\n')
print (b)

